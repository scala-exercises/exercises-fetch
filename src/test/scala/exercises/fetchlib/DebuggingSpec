package exercises

import fetchlib._
import shapeless.HNil

import org.scalaexercises.Test
import org.scalatest.Spec
import org.scalatest.prop.Checkers

import org.scalacheck.Shapeless._

class DebuggingSpec extends Spec with Checkers
{

	import FetchTutorialHelper._
	import DebuggingSection._
	import Test._

	def `Debugging` =
		check(testSuccess(debugging _, List(1,2):: 3 :: List(1,2,3) :: HNil))
}
